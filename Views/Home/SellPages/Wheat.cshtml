<!DOCTYPE HTML>

<html>
	<head>
		<title>Информация</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="/css/main.css" />
		<noscript><link rel="stylesheet" href="/css/noscript.css" /></noscript>
	</head>
	<body class="is-preload">

		<!-- Header -->
			<header id="header">
				<a asp-action="Index.html" class="title">АГРО-М</a>
				<nav>
					<ul>
						<li><a asp-action="Index">Главная</a></li>
						<li><a asp-action="Sell" class="active">Продажа</a></li>
					</ul>
				</nav>
			</header>

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Main -->
					<section id="main" class="wrapper">
						<div class="inner">
							<h1 class="major"><div id="value">Пшеница</div></h1>

							<p>
								<form action="" method="post">
									<select id="wheatSelect" name="wheatSelect">
										<option disabled selected>Сорт:</option>												
										<option value="1">1 Сорт</option>
										<option value="2">2 Сорт</option>
										<option value="3">3 Сорт</option>
										<option value="4">4 Сорт</option>
									</select> 
									<br>
									<br>
									<select id="regionSelect" name="regionSelect">
										<option disabled selected>Экономический регион:</option>												
										<option value="north">Северный</option>
										<option value="volga">Поволжский</option>
										<option value="ural">Уральский</option>
										<option value="center">Центральный</option>
									</select> 
									<br>
									
									<a>Обьем(т.)</a>
									<input name="valueOf" type="text">

									<br>

									<a>Цена(т.)</a>
									<input name="priceOf" type="text">
									
									<br>

									<input type="submit">
								</form>

							</p>
							<!-- <p>Описание</p> -->
						</div>
					</section>

			</div>

		<!-- Footer -->
			<footer id="footer" class="wrapper alt">
				<div class="inner">
					<ul class="menu">
						<li>&copy; Untitled. All rights reserved.</li>
					</ul>
				</div>
			</footer>

		<!-- Scripts -->
			<script src="/js/jquery.min.js"></script>
			<script src="/js/jquery.scrollex.min.js"></script>
			<script src="/js/jquery.scrolly.min.js"></script>
			<script src="/js/browser.min.js"></script>
			<script src="/js/breakpoints.min.js"></script>
			<script src="/js/util.js"></script>
			<script src="/js/main.js"></script>

	</body>
</html>